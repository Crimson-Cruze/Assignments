#include<stdio.h>

int main()
{
	int n,r,remainder,original;
	printf("enter an integer : ");
	scanf("%d",&n);
	
	original=n;
	while(n!=0)
	{
		remainder=n%10;
		r=r*10+remainder;
		n=n/10;
	}
	if(original==r)
	printf("This number is a palindrome");
	else
	printf("This number is not a palindrome");
	
	return 0;
}
